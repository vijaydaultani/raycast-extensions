{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "things-completed-yesterday",
  "title": "Things Completed Tasks",
  "description": "View tasks you completed today or yesterday in Things 3",
  "icon": "command-icon.png",
  "author": "vijaydaultani",
  "categories": ["Productivity"],
  "license": "MIT",
  "commands": [
    {
      "name": "show-completed-yesterday",
      "title": "Show Completed Yesterday",
      "subtitle": "Things",
      "description": "View tasks you completed yesterday",
      "mode": "view"
    },
    {
      "name": "show-completed-today",
      "title": "Show Completed Today",
      "subtitle": "Things",
      "description": "View tasks you completed today",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.103.5",
    "@raycast/utils": "^2.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^8.51.0",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "eslintConfig": {
    "root": true,
    "extends": ["@raycast"]
  },
  "platforms": ["macOS"]
}
